<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="#0090d3"
        android:orientation="vertical" >

        <TextView
            android:id="@+id/titlebarnewborn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:text="@string/new_born"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textColor="@color/white" />

        <TextView
            android:id="@+id/editprofile"
            android:layout_width="wrap_content"
            android:layout_height="50dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="10dp"
            android:gravity="center"
            android:text="@string/edit_profile"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textColor="@color/white" />
    </RelativeLayout>

  <RelativeLayout
        android:id="@+id/pinposspace"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#00aeef" >

        <RelativeLayout
            android:id="@+id/pinpos2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >

            <ImageView
                android:id="@+id/pin2"
                android:layout_width="14dp"
                android:layout_height="25dp"
                android:src="@drawable/goon_ipad_calendar_point01" />
        </RelativeLayout>
    </RelativeLayout>

    <LinearLayout
        android:id="@+id/ProgressSpace"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#00aeef"
        android:orientation="vertical" >

        <LinearLayout
            android:id="@+id/linearLayout3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal" >

            <LinearLayout
                android:id="@+id/showProgressLayout1"
                android:layout_width="60dp"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:orientation="vertical" >

                <TextView
                    android:id="@+id/showProgress2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="150\nDays"
                    android:textColor="#ffffff" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="20dp"
                android:layout_weight="0.41"
                android:orientation="vertical" >

                <LinearLayout
                    android:id="@+id/frame2"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:background="#464646"
                    android:orientation="horizontal" >

                    <LinearLayout
                        android:id="@+id/progressbar"
                        android:layout_width="20dp"
                        android:layout_height="match_parent"
                        android:background="#94dc2d"
                        android:orientation="vertical" >
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/showFullProgressLayout2"
                android:layout_width="60dp"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:orientation="vertical" >

                <TextView
                    android:id="@+id/fulldays2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="280\nDays"
                    android:textColor="#ffffff" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/showProgressLayout2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="10dp"
            android:layout_marginTop="6dp"
            android:orientation="vertical" >

            <TextView
                android:id="@+id/wombage2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_weight="1"
                android:text="●  Womb Age 130 days"
                android:textColor="#ffffff" />
        </LinearLayout>
    </LinearLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:gravity="center" >

        <TableRow
            android:id="@+id/tableRow1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center" >

            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="3dp"
                android:gravity="center" >

                <ImageView
                    android:id="@+id/boxtimedaybg"
                    android:layout_width="70dp"
                    android:layout_height="70dp"
                    android:src="@drawable/goon_ipad_new_born_box_time" />

                <ImageView
                    android:id="@+id/boxtimeday"
                    android:layout_width="70dp"
                    android:layout_height="70dp"
                    android:src="@drawable/goon_ipad_new_born_box_time" />

                <TextView
                    android:id="@+id/day"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:layout_centerVertical="true"
                    android:text="280"
                    android:textAppearance="?android:attr/textAppearanceLarge" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="3dp"
                android:gravity="center" >

                <ImageView
                    android:id="@+id/boxtimedaybg1"
                    android:layout_width="70dp"
                    android:layout_height="70dp"
                    android:src="@drawable/goon_ipad_new_born_box_time" />

                <ImageView
                    android:id="@+id/boxtimehour"
                    android:layout_width="70dp"
                    android:layout_height="70dp"
                    android:src="@drawable/goon_ipad_new_born_box_time" />

                <TextView
                    android:id="@+id/hour"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:layout_centerVertical="true"
                    android:text="00"
                    android:textAppearance="?android:attr/textAppearanceLarge" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="3dp"
                android:gravity="center" >

                <ImageView
                    android:id="@+id/boxtimedaybg2"
                    android:layout_width="70dp"
                    android:layout_height="70dp"
                    android:src="@drawable/goon_ipad_new_born_box_time" />

                <ImageView
                    android:id="@+id/boxtimemin"
                    android:layout_width="70dp"
                    android:layout_height="70dp"
                    android:src="@drawable/goon_ipad_new_born_box_time" />

                <TextView
                    android:id="@+id/minute"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:layout_centerVertical="true"
                    android:text="00"
                    android:textAppearance="?android:attr/textAppearanceLarge" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="3dp"
                android:gravity="center" >

                <ImageView
                    android:id="@+id/boxtimedaybg3"
                    android:layout_width="70dp"
                    android:layout_height="70dp"
                    android:src="@drawable/goon_ipad_new_born_box_time" />

                <ImageView
                    android:id="@+id/boxtimesec"
                    android:layout_width="70dp"
                    android:layout_height="70dp"
                    android:src="@drawable/goon_ipad_new_born_box_time" />

                <TextView
                    android:id="@+id/second"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:layout_centerVertical="true"
                    android:text="00"
                    android:textAppearance="?android:attr/textAppearanceLarge" />
            </RelativeLayout>
        </TableRow>
    </RelativeLayout>

    <ImageView
        android:id="@+id/babyprogress"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_gravity="center"
        android:layout_marginTop="30dp"
        android:src="@drawable/goon_ipad_new_born_baby03" />

    <Button
        android:id="@+id/congrats"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:text="Congratulations" />

</LinearLayout>